  "main": ".vite/build/main.js",
  "scripts": {
    "generate-routes": "tsr generate",
    "generate-schema": "cross-env NODE_ENV=development npx drizzle-kit generate",
    "sync-schema": "set ELECTRON_RUN_AS_NODE=true && cross-env NODE_ENV=development electron ./node_modules/drizzle-kit/bin.cjs push",
    "watch-routes": "tsr watch",
    "dev": "npm run watch-routes && vite dev",
    "build": "npm run generate-routes && vite build",
    "start": "concurrently \"npm run watch-routes\" \"electron-forge start\"",
    "package": "electron-forge package",
    "make": "electron-forge make",
    "publish": "electron-forge publish",
    "lint": "eslint --ext .ts,.tsx ."
